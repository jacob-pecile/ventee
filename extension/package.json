{
  "name": "ventee",
  "version": "0.0.1",
  "description": "Chrome extension for people who need a place to vent",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --env.dev --open --watch",
    "build": "parcel build index.html",
    "build:prod": "./node_modules/.bin/tsc && \"./node_modules/.bin/webpack\" -p --env.prod",
    "transpile": "./node_modules/.bin/tsc",
    "uninstall-webpack-dependencies": "npm uninstall @babel/core @babel/preset-env @types/react @types/react-dom babel-loader babel-polyfill babel-preset-react ts-loader webpack webpack-cli webpack-dev-server typescript --save-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacob-pecile/ventee.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacob-pecile/ventee/issues"
  },
  "homepage": "https://github.com/jacob-pecile/ventee#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "confetti-js": "0.0.18",
    "gotta-go-form": "^1.0.21",
    "react": "16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
