{
  "manifest_version": 2,
  "name": "Ventee",
  "description": "Chrome extension for people who need a place to vent",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "https://9cxulb4px0.execute-api.us-east-1.amazonaws.com/*"
  ]
}