{
  "manifest_version": 2,
  "name": "Ventee",
  "description": "Chrome extension for people who need a place to vent",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}